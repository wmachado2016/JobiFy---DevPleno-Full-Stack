
        <%- include('header') %>
            
       
        <h2>Admin Vagas: <span><a href="/admin/vagas/nova">Nova vaga</a></span></h2>
        
        <div>
            <ul>
                <% vagas.forEach(vaga => { %>
                <li>
                    <%- vaga.titulo %> |
                    <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a>
                    <a href="/admin/vagas/editar/<%- vaga.id %>">Editar</a>
                </li>
                <% }) %>
            </ul>
        </div>
  <%- include('footer') %>
