
<%- include('header') %>
    

<h2>Admin Categorias: <span><a href="/admin/categorias/nova">Nova Categoria</a></span></h2>

<div>
    <ul>
        <% categorias.forEach(categoria => { %>
        <li>
            <%- categoria.categoria %> |
            <a href="/admin/categorias/delete/<%- categoria.id %>">Excluir</a>
            <a href="/admin/categorias/editar/<%- categoria.id %>">Editar</a>
        </li>
        <% }) %>
    </ul>
</div>
<%- include('footer') %>
